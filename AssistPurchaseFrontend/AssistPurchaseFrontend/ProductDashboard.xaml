<UserControl x:Class="AssistPurchaseFrontend.ProductDashboard"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AssistPurchaseFrontend"
             mc:Ignorable="d" 
             d:DesignHeight="490" d:DesignWidth="450">
    <Grid Background="#0474ac">
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="40"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" x:Name="ImageGrid" Background="Transparent"></Grid>
        <ToggleButton Content="ProductId" x:Name="ProductId" Grid.Row="1" Style="{StaticResource ResourceKey=ToggleButtonStyle}"></ToggleButton>
    </Grid>
</UserControl>
